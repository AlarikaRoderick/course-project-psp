<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.controllers.AdminAddSessionPageController">
    <children>
        <Label layoutX="210.0" layoutY="35.0" text="Добавление сеанса">
            <font>
                <Font name="Times New Roman" size="21.0" />
            </font>
        </Label>
        <TextField fx:id="sessionHallField" layoutX="307.0" layoutY="126.0" prefHeight="27.0" prefWidth="235.0" promptText="номер зала" />
        <TextField fx:id="sessionHourField" layoutX="307.0" layoutY="173.0" prefHeight="27.0" prefWidth="106.0" promptText="часы" />
        <TextField fx:id="sessionMinuteField" layoutX="426.0" layoutY="173.0" prefHeight="27.0" prefWidth="120.0" promptText="минуты" />
        <Label layoutX="93.0" layoutY="86.0" text="Дата сеанса">
            <font>
                <Font name="Times New Roman" size="21.0" />
            </font>
        </Label>
        <Label layoutX="93.0" layoutY="128.0" text="Номер зала">
            <font>
                <Font name="Times New Roman" size="21.0" />
            </font>
        </Label>
        <Label layoutX="93.0" layoutY="173.0" text="Время сеанса">
            <font>
                <Font name="Times New Roman" size="21.0" />
            </font>
        </Label>
        <Button fx:id="addButton" onAction="#addSession" layoutX="376.0" layoutY="264.0" mnemonicParsing="false" text="Добавить">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Button>
        <Button fx:id="clearButton" layoutX="123.0" layoutY="264.0" mnemonicParsing="false" onAction="#clear" text="Очистить">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Button>
        <Button fx:id="backButton" onAction="#back" layoutX="44.0" layoutY="31.0" mnemonicParsing="false" text="Назад">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Button>
      <DatePicker fx:id="sessionDateField" layoutX="308.0" layoutY="85.0" prefHeight="27.0" prefWidth="235.0" promptText="Дата сеанса" />
    </children>
</AnchorPane>
